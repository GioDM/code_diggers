<%- include("../header.ejs") -%>
<div>
    <img class="set-minifig-img" src="<%=setImg%>" alt="img-set">
    <div>
        <div>
            Code Set: <%=setCode%>
            Volledige Code: <%=fullSetCode%>
        </div>
    </div>
</div>
<table>
    <tr>
        <% for (let i = 0; i < setMinifigs.results.length; i++) { %>
            <th><%=setMinifigs.results[i].set_name%></th>
        <% } %>
    </tr>
    <tr>
        <% for (let i = 0; i < setMinifigs.results.length; i++) { %>
            <td><a href="/legomasters/minifig/<%=setMinifigs.results[i].set_num%>?"><img class="set-minifig-img" src="<%=setMinifigs.results[i].set_img_url%>" alt="img-minifig" ></a></td>
        <% } %>
    </tr>
</table>
<%- include("../footer.ejs") -%>