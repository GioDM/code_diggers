<%- include("../header.ejs") -%>
<center>
    <h1>Overzicht Blacklist</h1>
    <table style ="width: 75%" id="BlacklistTable">
        <tr>
            <th>Picture minifig</th>
            <th>Naam (code)</th>
            <th>Reden</th>
            <th>Edit</th>
        </tr>
        <%for (let i = 0; i < result.length; i++) { %>
            <tr>
                <td scope="row"><img alt="imageMinifig" src="<%=result[i].urlImageMinifig%>"></td>
                <td><%=result[i].name%> (<%=result[i].codeMinifig%>)</td>
                <td><%=result[i].reason%></td>
                <td><img alt="editPen" src="data:image/png;base64,"/></td>
            </tr>
        <% } %>
        
    </table>
</center>
<%- include("../footer.ejs") -%>